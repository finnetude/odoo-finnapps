<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">


    <!-- Livre de paie-->
    <record id="report_hr_payroll_book_payrol" model="ir.actions.report">
        <field name="model">finn.hr.payroll.book</field>
        <field name="name">Livre de paie</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">finnapps_hr_account_reports.report_hr_payroll_book_pay</field>
