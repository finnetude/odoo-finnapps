<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="seq_certificate_val_id">
          <field name="name">Certificate Number</field>
          <field name="code">hr.contract.certificate</field>
          <field name="suffix">/%(year)s</field>
          <field name="padding">4</field>
          <field name="number_increment">1</field>
          <field name="number_next_actual">1</field>
        </record>

        <record model="ir.sequence" id="seq_attestation_val_id">
          <field name="name">Attestation Number</field>
          <field name="code">hr.contract.attestation</field>
          <field name="suffix">/%(year)s</field>
          <field name="padding">4</field>
          <field name="number_increment">1</field>
          <field name="number_next_actual">1</field>
        </record>
    </data>
</odoo>